<twig:Table:Row>
    <twig:Table:Cell>{{ chapter.title }}</twig:Table:Cell>
    <twig:Table:Cell>{{ chapter.subTitle }}</twig:Table:Cell>
    <twig:Table:Cell>{{ chapter.releasedAt|date('d F Y') }}</twig:Table:Cell>
    <twig:Table:Cell class="text-right">
        <twig:Chapter:DownloadButton :chapter="chapter" />
    </twig:Table:Cell>
</twig:Table:Row>
